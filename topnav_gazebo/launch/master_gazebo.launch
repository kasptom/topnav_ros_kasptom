<launch>
    <node name="capo_laser_scan_transceiver" pkg="topnav_gazebo" type="capo_laser_scan_transceiver"/>
    <node name="aruco_detector" pkg="topnav_aruco_detection" type="aruco_detector"/>
    <group ns="capo">
        <node name="ticker"
              pkg="topnav_capo2"
              type="capo2_ticker.py"
              respawn="false" output="screen">
            <param name="ticker_publish_rate_hz" value="10"/>
        </node>
    </group>
</launch>
